<odoo>
    <record id="rent_rule_user_contract" model="ir.rule">
        <field name="name">Rent User can only view active contracts</field>
        <field name="model_id" ref="model_rent_contract"/>
        <field name="domain_force">[('active', '=', True)]</field>
        <field name="groups" eval="[(4, ref('group_rent_user'))]"/>
    </record>

    <record id="rent_rule_admin_contract" model="ir.rule">
        <field name="name">Rent Admin can view all contracts</field>
        <field name="model_id" ref="model_rent_contract"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('group_rent_admin'))]"/>
    </record>
</odoo>
