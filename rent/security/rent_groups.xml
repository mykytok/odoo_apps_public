<odoo>
    <data noupdate="1">

        <record id="category_rent" model="ir.module.category">
            <field name="name">Rent</field>
            <field name="description">User access levels for Rent</field>
            <field name="sequence">1</field>
        </record>

        <record id="group_rent_user" model="res.groups">
            <field name="name">Rent User</field>
            <field name="category_id" ref="category_rent"/>
            <field name="users" eval="[Command.link(ref('base.user_root')), Command.link(ref('base.user_admin'))]"/>
            <field name="comment">User of Rent</field>
        </record>

        <record id="group_rent_admin" model="res.groups">
            <field name="name">Rent Admin</field>
            <field name="category_id" ref="category_rent"/>
            <field name="implied_ids" eval="[(4, ref('group_rent_user'))]"/>
            <field name="comment">Admin of Rent</field>
        </record>

    </data>
</odoo>