<odoo>
    <record model="ir.ui.view" id="view_rent_rental_object_group_tree">
        <field name="name">rent.rental.object.group.list</field>
        <field name="model">rent.rental.object.group</field>
        <field name="arch" type="xml">
            <list string="Rental object groups"
                multi_edit="1"
                sample="1">
                <field name="name"/>
            </list>
        </field>
    </record>
